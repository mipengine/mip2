{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "esModuleInterop": true,
    "sourceMap": true
  }
}