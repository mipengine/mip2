{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8",
            "iOS > 7",
            "android > 4.4"
          ]
        }
      }
    ],
    "vue"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "absoluteRuntime": "@babel/runtime"
      }
    ]
  ],
  "ignore": ["dist/*.js"]
}
